{
	"manifest_version": 3,
	"name": "Unsplash New Tab",
	"version": "0.1.0",
	"incognito": "split",
	"description": "Show your photos, your liked photos, or your friend's photos from Unsplash inside Chrome when you open a new tab.",
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"permissions": [
		"tabs",
		"scripting",
		"storage"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_title": "Unsplash New Tab"
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"chrome_url_overrides": {
		"newtab": "newTab.html"
	}
}
